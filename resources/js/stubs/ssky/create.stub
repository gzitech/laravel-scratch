    created() {
        
        if (this.errors && (typeof this.errors === 'object')) {
            this.#snakeStudlyName#CreateForm.setErrors(this.errors);
        }
    },


    methods: {

        validate#studlyName#CreateForm: function (event) {

            this.#snakeStudlyName#CreateForm.startProcessing();
            
            if(!this.#snakeStudlyName#CreateForm.validate({
                #create.validate.item#
            })) {
                event.preventDefault();
                this.#snakeStudlyName#CreateForm.finishProcessing();
            }
        },
    },


